{
    "name": "arkx/ark-eloquent",
    "description": "An ARK Blockchain bridge for Laravel.",
    "keywords": ["laravel", "eloquent", "database", "ark", "blockchain", "crypto", "currency"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.me",
        "homepage": "https://brianfaust.me"
    }],
    "require": {
        "php": "^7.1",
        "arkecosystem/crypto": "dev-develop",
        "illuminate/database": "^5.6",
        "illuminate/support": "^5.6"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "ArkX\\Eloquent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArkX\\Tests\\Eloquent\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "ArkX\\Eloquent\\EloquentServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
